      <%- include('partials/header')%>
    <main>
        <h2>MY ADDRESS</h2>
        <div class="dashboard-container">
            <aside>
                <ul>
                    <a href="mydashboard" target="_self" class="blackanchor"> <li>Account Dashboard</li></a>
                    <li>Account Information</li>
                    <li class="active">Address Book</li>
                    <li>My Orders</li>
                    <li>My Wishlist</li>
                </ul>
            </aside>
            <section class="address-details">
                <h3>ACCOUNT INFORMATION</h3>
                <div class="info-grid">
                    <div class="info-column">
                        <h4>Contact Information</h4>
                        <p>Akshita</p>
                        <p>akshita@gmail.com</p>
                        <button class="edit-btn">Edit</button>
                        <button class="change-password-btn">Change Password</button>
                    </div>
                    <div class="info-column">
                        <h4>Newsletters</h4>
                        <p>You don't subscribe to our newsletter.</p>
                        <button class="edit-btn">Edit</button>
                    </div>
                </div>
                <h3>ADDRESS BOOK</h3>
                <div class="address-grid">
                    <div class="address-column">
                        <h4>Default Billing Address</h4>
                        <p>You have not set a default billing address.</p>
                        <button class="edit-btn">Edit Address</button>
                    </div>
                    <div class="address-column">
                        <h4>Default Shipping Address</h4>
                        <p>You have not set a default shipping address.</p>
                        <button class="edit-btn">Edit Address</button>
                    </div>
                </div>
            </section>
        </div>
    </main>
